{
  "name": "amplitude-browser-sdk-gtm-template",
  "packageManager": "pnpm@4.2.0",
  "private": true,
  "version": "1.0.0",
  "description": "The amplitude GTM template with @amplitude/analytics-browser internal",
  "scripts": {
    "test": "jest",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "build": "node scripts/build-template.js && node scripts/generate-types.js",
    "sync": "node scripts/sync-template.js",
    "dev": "pnpm setup-env && vite serve",
    "prepare": "husky",
    "setup-env": "node scripts/setup-env.js",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "type-check": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/amplitude/amplitude-browser-sdk-gtm-template.git"
  },
  "author": "Dan Graham",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/amplitude/amplitude-browser-sdk-gtm-template/issues"
  },
  "homepage": "https://github.com/amplitude/amplitude-browser-sdk-gtm-template#readme",
  "dependencies": {
    "yaml-lint": "^1.7.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.28.3",
    "@playwright/test": "^1.55.1",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "ejs": "^3.1.10",
    "eslint": "^8.57.0",
    "husky": "^9.1.7",
    "jest": "^30.1.3",
    "jest-environment-jsdom": "^30.1.2",
    "ts-jest": "^29.4.4",
    "typescript": "^5.0.0",
    "vite": "^7.1.6"
  }
}
